<body style="background-color: #999999;">
	
	<div class="limiter">
		<div class="container-login100">
			<div class="login100-more" style="background-image: url('images/flight6.jpg');"></div>

			<div class="wrap-login100 p-l-50 p-r-50 p-t-72 p-b-50">
				<form class="login100-form validate-form" action="Customer" method="post">
					<span class="login100-form-title p-b-59">
						Sign Up
					</span>
					<div class="wrap-input100 validate-input" data-validate="Name is required">
						<span class="label-input100">Full Name</span>
						<input class="input100" type="text" name="name" placeholder="Name..." id="f" onkeyup="return check1()" required>
						<span id="zero"></span>
						<span class="focus-input100"></span>
					</div>

					<div class="wrap-input100 validate-input" data-validate = "Valid email is required: ex@abc.xyz">
						<span class="label-input100">Email</span>
						<input class="input100" type="email" name="email" placeholder="Email address..." required>
						<span class="focus-input100"></span>
					</div>

					<div class="wrap-input100 validate-input" data-validate="Username is required">
						<span class="label-input100">Username</span>
						<input class="input100" type="text" name="username" placeholder="Username..." required>
						<span class="focus-input100"></span>
					</div>

					<div class="wrap-input100 validate-input" data-validate = "Password is required">
						<span class="label-input100">Password</span>
						<input class="input100" type="password" id="pass"name="pass" placeholder="*************" required>
						<span class="focus-input100"></span>
					</div>
					
					<div class="wrap-input100 validate-input" data-validate = "Repeat Password is required">
						<span class="label-input100">Repeat Password</span>
						<input class="input100" type="password" name="repeat-pass" id="repeat-pass" placeholder="*************" onkeyup="return check()" required><span id="message"></span>						
						<span class="focus-input100"></span>
						<span id="one"></span>
						
					</div>

					<div class="container-login100-form-btn">
						<div class="wrap-login100-form-btn">
							<div class="login100-form-bgbtn"></div>
							<button class="login100-form-btn" name="s1" value="signup">
								Sign Up
							</button>
						</div>
			</form>
			<script>
			function check(){
				var x=document.getElementById("repeat-pass").value;
				var y=document.getElementById("pass").value;
				var a;
				<%System.out.print(3); %>
				if(x!=y){
					a="password not ok";
					document.getElementById("one").innerHTML="passwords has to be same";
					return false;
				}
				else{
					a="password ok";
					document.getElementById("one").innerHTML="";
					return true;
				}
			}
			function check1(){
				var x=document.getElementById("f").value;
				var y=false;
				
				if(typeof(x)=="string")
					{y=true;
						document.getElementById("zero").innerHTML="";
						return true;
					}
				else{
					document.getElementById("zero").innerHTML="name has to be string";
					return false;
					}
			}
			</script>

						<a href="signin.jsp" class="dis-block txt3 hov1 p-r-30 p-t-10 p-b-10 p-l-30">
							Sign in
							<i class="fa fa-long-arrow-right m-l-5"></i>
						</a>
					</div>
				</form>
			</div>
		</div>
	</div>
	
